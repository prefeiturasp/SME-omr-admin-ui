<%
    Helper.Pagination.SetUp(Pagination.CurrentPage, Pagination.ItemsPerPage, Pagination.TotalItems, Query);
%>
<% if (Helper.Pagination.HasPager) { %>
<ul class="paginacao pull-right">
    <li class="nav">
        <% if (Helper.Pagination.HasPreviews) { %> <a href="<%= Helper.Pagination.Previews %>"><i class="material-icons md-24">chevron_left</i></a><% } %>
    </li>
    <li class="text-center"><%= Helper.Pagination.FirstItem %> - <%= Helper.Pagination.LastItem %> de <%= Helper.Pagination.TotalItems %></li>
    <li class="nav">
        <% if (Helper.Pagination.HasNext) { %><a href="<%= Helper.Pagination.Next %>"><i class="material-icons md-24">chevron_right</i></a><% } %>
    </li>
</ul>
<% } %>